{
  "name": "express-typescript-template",
  "version": "1.0.3",
  "description": "Template for NodeJS Backend using Typescript and ExpressJS",
  "main": "dist/app.js",
  "repository": "https://github.com/HossamMohsen7/express-typescript-template",
  "author": "Hossam Mohsen",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=24",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "cross-env NODE_ENV=development tsx watch src/app.ts",
    "test": "tsc && node dist/test.js",
    "build": "prisma generate && tsc && pnpm run build",
    "db": "prisma db push",
    "studio": "prisma studio"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/hpp": "^0.2.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/pg": "^8.16.0",
    "cross-env": "^10.1.0",
    "prisma": "^7.3.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "5.2.1",
    "filenamify": "^7.0.1",
    "formidable": "^3.5.4",
    "got": "^14.6.6",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.3",
    "luxon": "^3.7.2",
    "morgan": "^1.10.1",
    "nanoid": "^5.1.6",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.2.2",
    "slugify": "^1.6.6",
    "uuid": "^13.0.0",
    "zod": "^4.3.6",
    "zod-express-validator": "^0.0.6",
    "zod-validation-error": "^5.0.0"
  }
}